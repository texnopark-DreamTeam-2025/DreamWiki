{
  "name": "dream-wiki",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --mode=production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "openapi-ts": "openapi-ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@gravity-ui/icons": "^2.16.0",
    "@gravity-ui/navigation": "^3.7.2",
    "@hey-api/client-axios": "^0.9.1",
    "@hey-api/openapi-ts": "^0.82.5",
    "@monaco-editor/react": "^4.7.0",
    "axios": "^1.12.2",
    "monaco-editor": "^0.54.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.3",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@gravity-ui/uikit": "^7.23.0",
    "@types/node": "^24.6.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/whatwg-fetch": "^0.0.33",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc --noEmit --skipLibCheck"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
